/* Device tree overlay for nRF52840 Dongle - use existing storage partition for NVS */

/ {
	chosen {
		/* Use the existing storage partition for NVS */
		zephyr,storage = &storage_partition;
	};
};

/* Increase CDC ACM TX buffer to accommodate full grade table JSON output (~2500 bytes) */
&board_cdc_acm_uart {
	tx-fifo-size = <4096>;
};
